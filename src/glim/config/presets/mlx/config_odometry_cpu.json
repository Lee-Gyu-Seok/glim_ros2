{
  "odometry_estimation": {
    "so_name": "libodometry_estimation_cpu.so",
    "initialization_mode": "LOOSE",
    "initialization_window_size": 3.0,
    "init_pose_damping_scale": 1e+16,
    "smoother_lag": 10.0,
    "use_isam2_dogleg": false,
    "isam2_relinearize_skip": 1,
    "isam2_relinearize_thresh": 0.1,
    "fix_imu_bias": false,
    // Registration type: "GICP", "VGICP", or "SmallGICP"
    "registration_type": "SmallGICP",
    "max_iterations": 10,
    "lru_thresh": 100,
    "target_downsampling_rate": 0.1,
    // GICP (iVox-based) params
    "ivox_resolution": 0.25,
    "ivox_min_dist": 0.05,
    // VGICP params (similar to GPU version for comparison)
    "vgicp_resolution": 0.2,
    "vgicp_voxelmap_levels": 2,
    "vgicp_voxelmap_scaling_factor": 2.0,
    // SmallGICP params
    "small_gicp_max_correspondence_distance": 2.0,
    "small_gicp_rotation_eps": 0.1,
    "small_gicp_translation_eps": 0.001,
    "small_gicp_voxel_resolution": 0.25,
    "save_imu_rate_trajectory": true,
    "num_threads": 8
  }
}

{
  "sub_mapping": {
    "so_name": "libsub_mapping.so",
    "enable_imu": false,
    "enable_optimization": true,
    "max_num_keyframes": 15,
    "keyframe_update_strategy": "DISPLACEMENT",
    "keyframe_update_min_points": 500,
    "keyframe_update_interval_rot": 1.57,
    "keyframe_update_interval_trans": 0.5,
    "max_keyframe_overlap": 0.7,
    "create_between_factors": true,
    "between_registration_type": "GICP",
    "registration_error_factor_type": "VGICP_GPU",
    "keyframe_randomsampling_rate": 1.0,
    "keyframe_voxel_resolution": 0.1,
    "keyframe_voxelmap_levels": 4,
    "keyframe_voxelmap_scaling_factor": 2.0,
    "submap_downsample_resolution": 0.08
    // "submap_voxel_resolution": 0.08 // 사용 안 함
  }
}

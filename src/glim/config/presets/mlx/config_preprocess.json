{
  /*** Preprocessing parameters for MLX LiDAR ***/
  // MLX: 56 x 192 = 10752 points per scan at 20Hz
  "preprocess": {
    "distance_near_thresh": 0.3,
    "distance_far_thresh": 100.0,
    "use_random_grid_downsampling": false,
    "downsample_resolution": 0.1,
    "random_downsample_target": 0,
    "random_downsample_rate": 0.5,
    "enable_outlier_removal": false,
    "outlier_removal_k": 10,
    "outlier_std_mul_factor": 1.0,
    "enable_cropbox_filter": false,
    "crop_bbox_frame": "lidar",
    "crop_bbox_min": [-1.0, -1.0, -1.0],
    "crop_bbox_max": [1.0, 1.0, 1.0],
    "k_correspondences": 50,
    "num_threads": 4
  }
}
